@model IEnumerable<ControleClientes.ViewModels.AgenciaViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Agências</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeAgencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Endereco)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomeAgencia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endereco)
        </td>
        <td>
            @Html.ActionLink("Detalhes", "Details", new { id=item.IdAgencia })
        </td>
    </tr>
}

</table>
